def reformatDate(date):
    # Split the date into its components
    date = date.split()
    # Create a dictionary to map the month to its number
    month_map = {"Jan": "01","Feb": "02","Mar": "03","Apr": "04","May": "05","Jun": "06","Jul": "07","Aug": "08","Sep": "09","Oct": "10","Nov": "11","Dec": "12"}
    # Reformat the date
    return date[2] + "-" + month_map[date[1]] + "-" + date[0][:-2].zfill(2)

print(reformatDate("20th Oct 2052")) # 2052-10-20
print(reformatDate("6th Jun 1933")) # 1933-06-06